public class MyInitials
{
	public static void main(String[] args) {
	System.out.println("zzzzzz    ssssss  ttttttttt");
	System.out.println("    z     s           t");
	System.out.println("   z      ssssss      t");
	System.out.println("  z            s      t");
	System.out.println(" z             s      t");
	System.out.println("zzzzzz    ssssss      t");
	}
}

